    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^27.2.5",
    "jest-extended": "^2.0.0",
    "prettier": "^2.3.2",
    "prisma": "^4.3.1",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "**/{!(*.module),}.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      "src/migrations/*",
      "src/.*\\.deprecatedspec\\.ts",
      "src/.*\\.ava\\.ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    }
  }
